{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/puisi",
      "dest": "/public/puisi/index.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
